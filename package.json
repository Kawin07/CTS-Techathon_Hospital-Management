{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "backend:install": "cd backend && pip install -r requirements.txt",
    "backend:init": "cd backend && python scripts/init_database.py",
    "backend:test": "cd backend && python test_db_connection.py",
    "backend:dev": "cd backend && python app.py",
    "db:init": "node --loader ts-node/esm src/scripts/initDatabase.ts",
    "db:schema": "node --loader ts-node/esm src/scripts/initDatabase.ts --schema-only",
    "db:sample": "node --loader ts-node/esm src/scripts/initDatabase.ts --data-only"
  },
  "dependencies": {
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@supabase/supabase-js": "^2.57.4",
    "chart.js": "^4.5.0",
    "dotenv": "^17.2.2",
    "framer-motion": "^10.18.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.344.0",
    "mysql2": "^3.15.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "recharts": "^3.2.1",
    "three": "^0.158.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/leaflet": "^1.9.12",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
